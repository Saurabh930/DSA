    //Inorder
   
   public static void inorder(Node root){
        if(root!=null){
            inorder(root.left);
            System.out.print(root.key+" ");
            inorder(root.right);    
        }
    } 
    
    
    
    //Preorder
    
        public static void preorder(Node root){
        if(root!=null){
            System.out.print(root.key+" ");
            preorder(root.left);
            preorder(root.right);    
        }
    } 
    
    //Postorder
    
    
        public static void postorder(Node root){
        if(root!=null){
            postorder(root.left);
            postorder(root.right);
            System.out.print(root.key+" ");    
        }
    } 
